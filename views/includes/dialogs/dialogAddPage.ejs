<div id="DialogAddPage" class="dialog" data-dialog-cancelable="true">
    <div class="dialog-background"></div>
    <div class="dialog-content">
        <h1 class="dialog-heading">Add New Page</h1>

        <form action="/user/createpage" method="post" autocomplete="off" data-post-text="Creating Page">
            <p class="input-heading">Page Name</p>
            <input type="text" name="pagename" placeholder="Page Name" required>

            <input id="ArtefactViewButtonEdit" type="submit" value="Create Page" class="button dialog-action">
        </form>
    </div>
</div>
