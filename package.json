{
  "name": "it-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "mocha",
    "start": "npm run serve:scss && node ./bin/www",
    "eslint": "./node_modules/.bin/eslint ./",
    "eslint:fix": "./node_modules/.bin/eslint ./ --fix",
    "serve": "npm-run-all -p serve:*",
    "serve:scss": "gulp sass",
    "serve:watch": "gulp sass:watch",
    "serve:run": "nodemon ./bin/www"
  },
  "dependencies": {
    "@google-cloud/storage": "^3.1.0",
    "async": "^3.1.0",
    "compression": "^1.7.4",
    "concat-stream": "^2.0.0",
    "connect-mongodb-session": "^2.1.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "ejs": "~2.5.7",
    "express": "~4.16.0",
    "express-session": "^1.16.2",
    "express-validator": "^6.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-sass": "^4.0.2",
    "http-errors": "~1.6.2",
    "mongoose": "^5.6.9",
    "morgan": "~1.9.0",
    "multer": "^1.4.2",
    "node-sass": "^4.12.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "sharp": "^0.23.0",
    "zip-stream": "^2.1.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.2.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}